<div class="wpc-contactUs-form wpc-contactUs-form-style3">
  <div class="row">
    
    <div class="col-xs-12">
      <h1>your feed!</h1>
    </div>

    <div ng-app="app">
      <div ng-controller="followeesCtrl" ng-init="setup()">
        <p>search followees: <input ng-model="followeeFilter"></p>
          <div ng-repeat="post in followees | filter: followeeFilter"> 
            <h3><a href="/posts/{{ post.id }}">{{ post.text }}</a></h3> 
            <h3><a href="/posts/{{ post.id }}">{{ post.address }}</a></h3> 
            <h4>by <a href="/foodies/{{ post.foodie_id }}">{{ post.foodie }}</a></h4>
          </div>
      </div>
    </div>

  </div>
</div>

